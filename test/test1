function exponent(a, b) {
    result = 1;
    for(i = 0; i < b; i = i+1) {
        result = result * a;
    }
    return result;
}

a = exponent(12);
b = 2 * 3;

while ( b > -3) {
    if (b > 0) {
        exponent(b - 1);
    }
    else {
        if (b % 2 == 0) {
            return 12;
        }
    }
}
